{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/manifest.webmanifest",
                "/*.css",
                "/*.js"
            ],
            "urls": [
                "https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.css",
                "https://stackpath.bootstrapcdn.com/**",
                "https://kit.fontawesome.com/b4f87717f0.js"

            ]

        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**",
                "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
            ]
        }
    }],
    "dataGroups": [{
            "name": "request",
            "urls": [
                "https://afsaval.agenciasur.cl/webservice/rest/requests"
            ],
            "cacheConfig": {
                "maxSize": 5000,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "assets",
            "urls": [
                "https://afsaval.agenciasur.cl/webservice/rest/assets"
            ],
            "cacheConfig": {
                "maxSize": 5000,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "locations",
            "urls": [
                "https://afsaval.agenciasur.cl/webservice/rest/catalog/locations"

            ],
            "cacheConfig": {
                "maxSize": 5000,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "costcenters",
            "urls": [
                "https://afsaval.agenciasur.cl/webservice/rest/catalog/costcenters"

            ],
            "cacheConfig": {
                "maxSize": 5000,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "species",
            "urls": [
                "https://afsaval.agenciasur.cl/webservice/rest/catalog/species"

            ],
            "cacheConfig": {
                "maxSize": 5000,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "species",
            "urls": [
                "https://afsaval.agenciasur.cl/webservice/rest/catalog/class"

            ],
            "cacheConfig": {
                "maxSize": 5000,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "centers",
            "urls": [
                "https://afsaval.agenciasur.cl/webservice/rest/location/centers"

            ],
            "cacheConfig": {
                "maxSize": 5000,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        }

    ]
}