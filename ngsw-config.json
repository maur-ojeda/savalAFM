{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/manifest.webmanifest",
                "/*.css",
                "/*.js"
            ],
            "urls": [
                "https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.css",
                "https://stackpath.bootstrapcdn.com/**",
                "https://kit.fontawesome.com/b4f87717f0.js"

            ]

        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**",
                "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
            ]
        }
    }],
    "dataGroups": [{
            "name": "request",
            "urls": [
                "https://afsaval.agenciasur.cl/*"
            ],
            "cacheConfig": {
                "strategy": "performance",
                "maxSize": 100,
                "maxAge": "10h",
                "timeout": "5s"

            }
        }

    ]
}